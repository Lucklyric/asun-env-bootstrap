  "git.addGBlameToVirtualText": true,
  "python.analysis.typeCheckingMode": "off",
  "python.formatting.provider": "yapf",
  "python.formatting.yapfArgs": ["--style", "google"],
  "python.linting.pylintArgs": [
    "--errors-only",
    "--generated-members=numpy.* ,torch.* ,cv2.* , cv.*"
